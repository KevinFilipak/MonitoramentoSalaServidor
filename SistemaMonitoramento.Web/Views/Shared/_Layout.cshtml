<!DOCTYPE html>

<html lang="en">

<head>
    <meta charset="utf-8" />

    <title>DEFAULT | Sistema</title>

    <meta name="viewport" content="width=device-width, initial-scale=1" />

    <link rel="apple-touch-icon" sizes="144x144" href="@Url.Content("~/images/icon/apple-touch-icon.png")">
    <link rel="icon" type="image/png" sizes="32x32" href="@Url.Content("~/images/icon/favicon-32x32.png")">
    <link rel="icon" type="image/png" sizes="16x16" href="@Url.Content("~/images/icon/favicon-16x16.png")">
    <link rel="mask-icon" href="@Url.Content("~/images/icon/safari-pinned-tab.svg")" color="#5bbad5">
    <meta name="msapplication-TileColor" content="#da532c">
    <meta name="theme-color" content="#ffffff">

    <link rel="stylesheet" href="https://fonts.googleapis.com/css?family=Poppins:300,400,500,600,700" />


    <link href="@Url.Content("~/assets/plugins/custom/leaflet/leaflet.bundle.css")" rel="stylesheet" type="text/css" />
    <link href="@Url.Content("~/assets/plugins/custom/datatables/datatables.bundle.css")" rel="stylesheet" type="text/css" />
    <link href="@Url.Content("~/assets/plugins/global/plugins.bundle.css")" rel="stylesheet" type="text/css" />
    <link href="@Url.Content("~/assets/css/style.bundle.css")" rel="stylesheet" type="text/css" />


    <script src="@Url.Content("~/assets/plugins/global/plugins.bundle.js")"></script>
    <script src="@Url.Content("~/assets/js/scripts.bundle.js")"></script>
    
    @Html.Partial("_Css")
</head>

<body id="kt_body" class="header-fixed header-tablet-and-mobile-fixed aside-enabled aside-fixed aside-primary-enabled aside-secondary-enabled" data-kt-aside-minimize="on">

    <div class="d-flex flex-column flex-root">
        <div class="page d-flex flex-row flex-column-fluid">

            @Html.Partial("_Menu")


            <div class="wrapper d-flex flex-column flex-row-fluid" id="kt_wrapper">

                @Html.Partial("_Header")

                <div class="d-flex flex-column flex-column-fluid">

                    <div class="content fs-6 d-flex flex-column-fluid" id="kt_content">

                        <div class="container">

                            @RenderBody()

                        </div>
                    </div>
                </div>

                @Html.Partial("_Footer")

            </div>
        </div>
    </div>

    <div id="kt_scrolltop" class="scrolltop" data-kt-scrolltop="true">
        <span class="svg-icon">
            <svg xmlns="http://www.w3.org/2000/svg" xmlns:xlink="http://www.w3.org/1999/xlink" width="24px" height="24px" viewBox="0 0 24 24" version="1.1">
                <g stroke="none" stroke-width="1" fill="none" fill-rule="evenodd">
                    <polygon points="0 0 24 0 24 24 0 24" />
                    <rect fill="#000000" opacity="0.5" x="11" y="10" width="2" height="10" rx="1" />
                    <path d="M6.70710678,12.7071068 C6.31658249,13.0976311 5.68341751,13.0976311 5.29289322,12.7071068 C4.90236893,12.3165825 4.90236893,11.6834175 5.29289322,11.2928932 L11.2928932,5.29289322 C11.6714722,4.91431428 12.2810586,4.90106866 12.6757246,5.26284586 L18.6757246,10.7628459 C19.0828436,11.1360383 19.1103465,11.7686056 18.7371541,12.1757246 C18.3639617,12.5828436 17.7313944,12.6103465 17.3242754,12.2371541 L12.0300757,7.38413782 L6.70710678,12.7071068 Z" fill="#000000" fill-rule="nonzero" />
                </g>
            </svg>
        </span>
    </div>



    <script src="@Url.Content("~/assets/plugins/custom/datatables/datatables.bundle.js")"></script>
    <script src="@Url.Content("~/content/moments/moment.js")"></script>
    <script src="@Url.Content("~/content/moments/moment-with-locales.js")"></script>
    <script src="@Url.Content("~/content/numeral/numeral.js")"></script>
    <script src="@Url.Content("~/content/numeral/locales.js")"></script>
    <script src="@Url.Content("~/content/flatpickr/pt.js")"></script>
    <script src="@Url.Content("~/content/select2/select2-dropdownPosition.js")"></script>
    <script src="@Url.Content("~/content/select2/pt-BR.js")"></script>
    <script src="@Url.Content("~/content/typeahead/typeahead.bundle.js")"></script>

    @Html.Partial("_Scripts")


    @RenderSection("scripts", required: false)
</body>
</html>
